module zwetser::data

import mobl

entity TwitterUpdate {
  updateId         : Num
  screenName       : String
  iconUrl          : String
  text             : String
  mention          : Bool
  favorite         : Bool
  date             : DateTime
  lastUpdate       : DateTime
  conversationRoot : TwitterUpdate
  replyCount       : Num // only for root
  replies          : Collection<TwitterUpdate> (inverse: conversationRoot)
}