application calculator

title "Calculator"

import mobl

sync function doCalculation(n1 : Int, n2 : Int) : Int {
  return n1 * n2;
}

screen root() {
  header("Calculator")
  var n1 : Int = 2
  var n2 : Int = 2
  
  inputs {
    inputInt(n1)
    inputInt(n2)
    listitem { "Multiplied: " outputInt(doCalculation(n1, n2)) }
  }
}