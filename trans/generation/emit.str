module generation/emit

strategies

  emit-html =
    ?code
    ; current-html := <EmittedHtml <+ !"">
    ; rules ( EmittedHtml := <concat-strings> [current-html, code] )
    
  get-all-html =
       EmittedHtml
       ; rules ( EmittedHtml := "")
    <+ !""
    
  emit-js =
    ?code
    ; current-js := <EmittedJs <+ !"">
    ; rules ( EmittedJs := <concat-strings> [current-js, code] )
    
  get-all-js =
       EmittedJs
       ; rules ( EmittedJs := "")
    <+ !""