module lookup

imports
  include/MoBL
  check
  declare

strategies

  lookup-type :
    SimpleType(qid) -> <GetEntity> qid
  
  lookup-type :
    SimpleType(qid) -> <GetExternalType> qid
    
  lookup-type :
    GenericType(qid, t*) -> <GetExternalGenericType> (qid, <length> t*)
  
  lookup-template :
    (name, arg-type*) -> <GetTemplate> <id>
    
  get-all-types =
    <concat> [<all-keys-GetEntity>, <all-keys-GetExternalType>, <map(Fst)> <all-keys-GetExternalGenericType>]