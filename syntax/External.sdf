module External

imports Common

exports

  context-free syntax
    "external" "type" QId "{" "}"                   -> Definition {cons("ExternalType")}
    "external" "type" QId "<" {ID ","}* ">" "{" "}" -> Definition {cons("ExternalGenericType")}
    
    "external" "screen" QId "(" {FArg ","}* ")"     -> Definition {cons("ExternalType")}
    "external" "template" QId "(" {FArg ","}* ")"   -> Definition {cons("ExternalGenericType")}